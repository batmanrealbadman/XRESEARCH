{
  "version": 2,
  "builds": [
    {
      "src": "api/auth/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}